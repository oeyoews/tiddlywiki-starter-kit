{
  "workbench.editor.customLabels.patterns": {
    "**/plugins/**/plugin.info": "${dirname} - plugin.info",
    // "**/plugins/**/tiddlers/app.js": "${dirname} - app.js",
    // "**/plugins/**/widget.js": "${dirname} - widget.js",
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "**/files/**/*.vue": "html",
    "*.info": "json",
    "*.meta": "tid",
    "*.tid": "tid",
    "*.files": "json",
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.experimental.configFile": "./plugins/oeyoews/tiddlywiki-tailwindcss-plus/tiddlers/tailwind.config.js",
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "files.autoSave": "off",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "tailwindCSS.emmetCompletions": true,
  "editor.formatOnSave": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, pnpm-workspace.yaml",
    "tiddlywiki.config.mjs": "tiddlywiki.info, tiddlywiki.config.mjs",
    "*.md": "${capture}.md, ${capture}.md.meta",
    "*.gitconfig": "${capture}.git*",
    "Dockerfile": "Dockerfile, .dockerignore, docker-compose.yml",
    "vercel.json": "vercel.json, netlify.toml",
  },
  "tailwindCSS.lint.invalidApply": "warning",
}