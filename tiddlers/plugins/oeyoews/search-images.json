"$:/showInfo/search/template": {
    "title": "$:/showInfo/search/template",
    "created": "20210911022445519",
    "description": "See $:/showInfo/search/SystemImage for an example of how to use this template.",
    "modified": "20220517090048586",
    "tags": "$:/showInfo/search $:/showInfo",
    "task": "doing",
    "type": "text/vnd.tiddlywiki",
    "text": "<span class=\"system-image-search\">\n<div class=\"search-list-container\">\n<$edit-text tiddler=<<qualify $:/temp/search>> field=\"text\" tag=\"input\" default=\"\" placeholder={{$:/showInfo/search/placeholder}} class=\"tc-edit-texteditor\"/>\n<$reveal state=<<qualify $:/temp/search>> type=\"nomatch\" text=\"\">\n<$button class=\"tc-btn-invisible\" tooltip=\"cancel search\"> \n<$action-setfield $tiddler=<<qualify $:/temp/search>> text=\"\"/>\n{{$:/core/images/close-button}}\n</$button>\n</$reveal>\n<$button class=\"tc-btn-invisible\" tooltip=\"advanced search\"> \n<$action-listops \n$tiddler=<<qualify $:/temp/advanced-search/show>> \n$field=\"text\" \n$subfilter=\"+[toggle[yes],[no]]\"/>\n{{$:/core/images/advanced-search-button}}\n</$button>\n</div>\n\n<$reveal default={{{ [[$:/temp/advanced-search/show]addsuffix<qualify>get[text]] }}} type=\"match\" text=\"yes\">\n<h3>Advanced search</h3>\n<div class=\"search-list-container\">\n<$edit-text tiddler=<<qualify $:/temp/filter>> field=\"text\" tag=\"input\" default=\"[search:[]]\" placeholder=\"write an additional filter here\" class=\"tc-edit-texteditor\"/>\n<$reveal state=<<qualify $:/temp/filter>> type=\"nomatch\" text=\"\">\n<$button class=\"tc-btn-invisible\">\n<$action-setfield $tiddler=<<qualify $:/temp/filter>> text=\"[search:[]]\"/>\n{{$:/core/images/close-button}}\n</$button>\n</$reveal>\n</div>\n</$reveal>\n\n\n<$set name=search tiddler=<<qualify $:/temp/search>> field=\"text\">\n<$set name=filter tiddler=<<qualify $:/temp/filter>> field=\"text\">\n<$set name=filter value={{{ [<filter>match[]then[+]else<filter>] }}}>\n\n<ul class=<<list-class>> style=<<list-style>> >\n<$list filter=\"[subfilter<search-list>filter<filter>]+[search:*<search>]\" variable=\"item\" template={{{ [<item-template>is[blank]then[$:/showInfo/search/titles]else<item-template>] }}} />\n</ul>\n\n</$set>\n</$set>\n</$set>\n</span>",
    "revision": "0",
    "bag": "default"
},

